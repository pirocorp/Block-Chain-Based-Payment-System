                    <div class="row">
                        <p class="col-sm-3 text-muted text-sm-right mb-0 mb-sm-3">Date of Birth</p>
                        <p class="col-sm-9">
                            @if (this.Model.DateOfBirth.Year < 1900)
                            {
                            @string.Empty
                            }
                            else
                            {
                            @this.Model.DateOfBirth.ToString("yyyy-MM-dd")
                            }
